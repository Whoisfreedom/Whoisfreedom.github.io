body
	float left
	.nav
		width 1920px
		height 41px
		background-color rgb(245,245,245)
		border 1px solid rgb(232,232,232)
		.nav-in
			width 1200px
			height 40px
			font-size 14px
			margin 0 auto
			.place
				position relative
				&:hover ul
					display block
				ul
					position absolute
					height 280px
					width 90px
					top 36	
					background-color rgb(232,232,232)
					display none
					z-index 10	
					li
						border 1px solid rgb(232,232,232)
						&:hover
							background-color white
						a
							display block
							width 90px
							height 38px
							line-height 40px
							padding 0 0
										
			div
				float left
				text-align center
				height 40px
				border 1px solid rgb(245,245,245)
				a
					line-height 38px
					padding 12px 19px
					color rgb(127,127,127)
				&:hover
					background-color white
					border 1px solid rgb(232,232,232)
				&:nth-of-type(5):hover a
					visibility visible
			.chang
				float right
			.price-search
				padding-right 10px
				padding-left 10px
				input[type=text]
					border 1px solid rgb(232,232,232)
					background-color white
					width 60px
					height 25px
					line-height 25px
				>a
					padding 4px 10px 
					background-color rgb(255,68,0)
					border-radius 3px
					color white
					margin-left 10px
					visibility hidden
					&:hover
						background-color rgb(248,40,0)
				&+div
					margin-left 20px
	.inside
		width 1190px
		height 	800px			
		margin 0 auto
		>div
			height 362px
			float left
			width 222px
			margin-right 20px
			background-color white
			margin-top 30px
			border 1px solid rgb(237,237,237)
			font-size 12px
			&:hover
				border 1px solid rgb(255,68,0)
			&:hover input[type=button]
				visibility visible
			&:nth-of-type(5)
				margin-right 0
			&:nth-of-type(10)
				margin-right 0
			>a:nth-of-type(1)
				display block
				width 220px 
				height 200px
				background-image url(../img/z2.jpg)
				background-size 220px 200px
				position relative
				overflow hidden
				&:hover div 
					top 170px
				div
					width 220px
					height 30px
					padding 0 0
					position absolute
					top 200px
					background-color rgb(255,68,0)
					opacity 0.8
					transition 1s					
					span
						display block
						width 50%
						height 30px
						border 1px solid rgb(255,68,0)
						float left
						padding 0 0
						margin 0 0 
						text-align center
						line-height 30px
						font-size 12px
						font-weight 400
						color white
						&:nth-of-type(2)
							float right
							border-left 1px solid white
			div:nth-of-type(1)
				padding 15px 5px
				span
					font-size 18px
					font-weight 700
					color rgb(255,68,0)
					font-family verdana,arial
			a:nth-of-type(2)
				display block
				padding 0 5px
				line-height 16px
				height 45px
				&:hover
					text-decoration underline
					color rgb(255,68,0)
			div:nth-of-type(2)
				padding 5px 5px 
				color rgb(136,136,136)
				height 30px
				line-height 20px
				a
					float left
					&:nth-of-type(1)
						padding 5px 4px 
						background-image url(../img/small.png)
						line-height 20px
						margin-top 5px
					&:nth-of-type(2)
						text-decoration underline
						padding 0px 4px 
						line-height 20px
						height 20px
						&:hover
							color rgb(255,68,0)
				span
					float right
					
			div:nth-of-type(3)
				width 100%
				padding 0px 5px
				height 20px
				img
					float left
				input 
					float right
					width 40px
					height 20px
					text-align center
					color white
					background-color rgb(255,68,0)
					cursor pointer
					visibility hidden
					&:hover
						background-color rgb(248,40,0)
						